{
    "name": "codecube/codecube",
    "type": "project",
    "description": "The CodeCube Framework: An easy & lightweight PHP framework.",
    "keywords": [
        "php",
        "framework",
        "codecube"
    ],
    "authors": [
        {
            "name": "Mahadi Hasan",
            "email": "bappychanting@outlook.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "codecube/codecube": "1.0.*",
        "phpmailer/phpmailer": "~6.0"
    },
    "autoload": {
        "psr-4":{
            "App\\": "app/",
            "Base\\": "base/Classes"
        },
        "files": [
            "base/Functions/Initials.php",
            "base/Functions/Inheritance.php",
            "base/Functions/View.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
